    "@angular/router": "^19.2.0",
    "@eslint/js": "^9.23.0",
    "@tailwindcss/postcss": "^4.1.1",
    "postcss": "^8.5.3",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.1.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
