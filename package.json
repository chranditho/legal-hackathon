{
  "name": "legal-hackathon",
  "version": "0.0.4",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write .",
    "prepare": "husky",
    "postversion": "git add package.json package-lock.json && git commit -m 'chore(release): bump version' || echo 'already committed' && git tag -f v$(node -p \"require('./package.json').version\") && git push && git push --tags --force"
